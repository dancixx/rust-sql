{
  "build": {
    "beforeBuildCommand": "trunk build",
    "beforeDevCommand": "trunk serve",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "RSQL",
    "version": "1.0.0"
  },
  "plugins": {
    "fs": {
      "scope": [
        "$APPDATA/*"
      ]
    },
    "shell": {
      "open": true
    }
  },
  "tauri": {
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.rsql.app",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 1080,
        "resizable": true,
        "title": "RSQL",
        "width": 1920
      }
    ]
  }
}